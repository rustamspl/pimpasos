{
    "background": {
        "scripts": ["background.js"]
        , "persistent": true
    },
     "browser_action": {
        "default_icon": "icon/128.png"
        , "default_popup": "popup/popup.html"
    }
    , "author": "https://github.com/rustamspl"
    , "description": "PIMPASOS"
    , "icons": {
        "16": "icon/16.png"
        , "48": "icon/48.png"
        , "128": "icon/128.png"
    }
   
    , "manifest_version": 2
    , "minimum_chrome_version": "45"
    , "name": "PIMPASOS"
    , "options_page": "/popup/opt.html"
    , "permissions": ["<all_urls>","tabs", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"
        , "downloads"
    ]
    , "short_name": "PIMPASOS"
    , "update_url": "https://127.0.0.1:99/"
    , "version": "0.1"
    , "web_accessible_resources": ["*"]
}
